<%# <h1>Trucks#new</h1> %>
<h1>New Food Truck</h1>

    <%= form_for @truck do |f| %>

        Name: <%= f.text_field :name %> </br>
        Money: <%= f.number_field :money %> </br>
        <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
    <%= f.submit "Register Food Truck" %>
<% end %>